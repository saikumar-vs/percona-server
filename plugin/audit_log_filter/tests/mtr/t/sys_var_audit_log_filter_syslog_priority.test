
SET @start_value = @@global.audit_log_filter_syslog_priority;

SET GLOBAL audit_log_filter_syslog_priority=LOG_INFO;
SELECT @@global.audit_log_filter_syslog_priority;

SET GLOBAL audit_log_filter_syslog_priority=LOG_ALERT;
SELECT @@global.audit_log_filter_syslog_priority;

SET GLOBAL audit_log_filter_syslog_priority=LOG_CRIT;
SELECT @@global.audit_log_filter_syslog_priority;

SET GLOBAL audit_log_filter_syslog_priority=LOG_ERR;
SELECT @@global.audit_log_filter_syslog_priority;

SET GLOBAL audit_log_filter_syslog_priority=LOG_WARNING;
SELECT @@global.audit_log_filter_syslog_priority;

SET GLOBAL audit_log_filter_syslog_priority=LOG_NOTICE;
SELECT @@global.audit_log_filter_syslog_priority;

SET GLOBAL audit_log_filter_syslog_priority=LOG_EMERG;
SELECT @@global.audit_log_filter_syslog_priority;

SET GLOBAL audit_log_filter_syslog_priority=LOG_DEBUG;
SELECT @@global.audit_log_filter_syslog_priority;

--error ER_WRONG_VALUE_FOR_VAR
SET GLOBAL audit_log_filter_syslog_priority=LOG_UNKNOWN;
SELECT @@global.audit_log_filter_syslog_priority;

--error ER_WRONG_VALUE_FOR_VAR
SET GLOBAL audit_log_filter_syslog_priority='';
SELECT @@global.audit_log_filter_syslog_priority;

--error ER_WRONG_VALUE_FOR_VAR
SET GLOBAL audit_log_filter_syslog_priority='str';
SELECT @@global.audit_log_filter_syslog_priority;

--error ER_WRONG_VALUE_FOR_VAR
SET GLOBAL audit_log_filter_syslog_priority=-1;
SELECT @@global.audit_log_filter_syslog_priority;

--error ER_WRONG_TYPE_FOR_VAR
SET GLOBAL audit_log_filter_syslog_priority=0.5;
SELECT @@global.audit_log_filter_syslog_priority;

SET GLOBAL audit_log_filter_syslog_priority = @start_value;
